{
  "name": "Lifekeyrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start_packager": "node_modules/react-native/packager/packager.sh",
    "test": "node node_modules/jest/bin/jest.js --coverage",
    "precommit": "node node_modules/eslint/bin/eslint.js eslint",
    "set_cfg": "./bin/set_cfg",
    "uninstall-apk": "for d in $(adb devices -l | sed '1d' | sed '$d' |  awk '{print $1}'); do adb -s $d uninstall com.lifekeyrn; done",
    "clear_packager_cache": "rm -rf $TMPDIR/react-native-packager-cache-*; rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "build_rn_android_docs": "./bin/build_rn_android_docs",
    "log_android_clean": "./bin/log_android_clean"
  },
  "dependencies": {
    "ansi-styles": "^2.2.1",
    "deepmerge": "^1.3.2",
    "fbjs": "^0.8.8",
    "global": "^4.3.1",
    "moment": "^2.17.1",
    "nachos-ui": "^0.1.0",
    "native-base": "~2.0.0",
    "react": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-native": "^0.42.0",
    "react-native-android-back-button": "^0.3.0",
    "react-native-camera": "^0.6.0",
    "react-native-cli": "^2.0.1",
    "react-native-dialogs": "0.0.19",
    "react-native-google-analytics-bridge": "^5.0.0",
    "react-native-qrcode": "^0.2.3",
    "react-native-svg": "^5.1.5",
    "react-native-vector-icons": "^4.0.0",
    "tcomb-form-native": "^0.6.4"
  },
  "devDependencies": {
     "babel-cli": "^6.3.17",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^1.9.0",
    "enzyme": "^2.8.0",
    "enzyme-to-json": "^1.5.0",
    "eslint": "^3.14.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-react-native": "^2.2.1",
    "jest": "^19.0.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.1"
  },
  "jest": {
    "preset": "react-native",
    "unmockedModulePathPatterns": [
      "<rootdir>/node_modules/react-native"
    ],
    "transformIgnorePatterns": [
  "node_modules/(?!react-native|lifekey-rn|react-native-button)/"
],
    "setupFiles": [
      "./jestconfig.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
  }
}
