{
  "name": "Lifekeyrn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "node node_modules/jest/bin/jest.js",
    "precommit": "node node_modules/eslint/bin/eslint.js eslint",
    "set_cfg": "./bin/set_cfg",
    "uninstall-apk": "for d in $(adb devices -l | sed '1d' | sed '$d' |  awk '{print $1}'); do adb -s $d uninstall com.lifekeyrn; done",
    "clear_packager_cache": "rm -rf $TMPDIR/react-native-packager-cache-*; rm -rf $TMPDIR/haste-map-react-native-packager-*",
    "build_rn_android_docs": "./bin/build_rn_android_docs"
  },
  "dependencies": {
    "ansi-styles": "^2.2.1",
    "fbjs": "^0.8.8",
    "global": "^4.3.1",
    "moment": "^2.17.1",
    "nachos-ui": "^0.1.0",
    "native-base": "^0.5.22",
    "react": "^15.4.2",
    "react-native": "^0.40.0",
    "react-native-android-back-button": "^0.3.0",
    "react-native-camera": "^0.6.0",
    "react-native-cli": "^2.0.1",
    "react-native-dialogs": "0.0.19",
    "react-native-google-analytics-bridge": "^5.0.0",
    "react-native-qrcode": "^0.2.3",
    "react-native-vector-icons": "^4.0.0",
    "tcomb-form-native": "^0.6.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-native": "^1.9.0",
    "eslint": "^3.14.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-react-native": "^2.2.1",
    "jest": "^17.0.3",
    "jest-react-native": "^17.0.3",
    "react-test-renderer": "^15.4.1"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-clone-referenced-element|tcomb-form-native)"
    ]
  }
}
